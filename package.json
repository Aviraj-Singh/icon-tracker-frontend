    "scripts": {
        "start": "node scripts/start.js",
        "start:mainnet": "REACT_APP_ENV=mainnet node scripts/start.js",
        "start:testnet": "REACT_APP_ENV=testnet node scripts/start.js",
        "start:testnet1": "REACT_APP_ENV=testnet1 node scripts/start.js",
        "start:custom": "REACT_APP_ENV=custom node scripts/start.js",
        "start:prep": "REACT_APP_ENV=prep node scripts/start.js",
        "start:qa": "REACT_APP_ENV=qa node scripts/start.js",
        "start:np": "REACT_APP_ENV=np node scripts/start.js",
        "build": "node scripts/build.js",
        "build:mainnet": "rm -rf build && REACT_APP_ENV=mainnet node scripts/build.js && zip -FSr build-mainnet.zip build",
        "build:testnet": "rm -rf build && REACT_APP_ENV=testnet node scripts/build.js && zip -FSr build-testnet.zip build",
        "build:testnet1": "rm -rf build && REACT_APP_ENV=testnet1 node scripts/build.js && zip -FSr build-testnet1.zip build",
        "build:custom": "rm -rf build && REACT_APP_ENV=custom node scripts/build.js && zip -FSr build-custom.zip build",
        "build:prep": "rm -rf build && REACT_APP_ENV=prep node scripts/build.js && zip -FSr build-prep.zip build",
        "build:qa": "rm -rf build && REACT_APP_ENV=qa node scripts/build.js && zip -FSr build-qa.zip build",
        "build:np": "rm -rf build && REACT_APP_ENV=np node scripts/build.js && zip -FSr build-np.zip build",
        "test": "node scripts/test.js --env=jsdom"
    },
    "devDependencies": {
        "worker-loader": "^2.0.0"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}",
            "!src/**/*.d.ts"
        ],
        "resolver": "jest-pnp-resolver",
        "setupFiles": [
            "react-app-polyfill/jsdom"
        ],
        "testMatch": [
            "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
            "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
        ],
        "testEnvironment": "jsdom",
        "testURL": "http://localhost",
        "transform": {
            "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
            "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
            "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
            "^.+\\.module\\.(css|sass|scss)$"
        ],
        "moduleNameMapper": {
            "^react-native$": "react-native-web",
            "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
        },
        "moduleFileExtensions": [
            "web.js",
            "js",
            "web.ts",
            "ts",
            "web.tsx",
            "tsx",
            "json",
            "web.jsx",
            "jsx",
            "node"
        ]
    },
    "babel": {
        "presets": [
            "react-app"
        ]
    },
    "eslintConfig": {
        "extends": "react-app"
    }
}
